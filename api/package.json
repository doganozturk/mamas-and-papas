    "index-delete": "curl -XDELETE 'http://localhost:9200/_all'",
    "indices": "node ./elasticsearch/indices.js"
  },
  "dependencies": {
    "bunyan": "1.8.10",
    "elasticsearch": "13.1.1",
    "lodash.get": "4.4.2",
    "restify": "4.3.0"
  },
  "devDependencies": {
    "nodemon": "1.11.0"
  }
}
