{
  "name": "vngrs-api",
  "version": "0.0.1",
  "main": "app.js",
  "author": "Doğan Öztürk",
  "engines": {
    "node": ">=6.11.0"
  },
  "private": true,
  "scripts": {
    "dev": "./node_modules/nodemon/bin/nodemon.js app.js",
    "dist": "npm run index-delete && npm run index && npm run indices && node app.js",
